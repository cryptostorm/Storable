<a href='<%= listing_path(listing.id) %>' style="overflow:hidden;"> 
	<div class="listing-icon" style="background-image: url('<%= listing.images.last ? listing.images.last.location : '' %>');">
		<div class="listing-icon-overlay">
			<div class="listing-overlay-wrapper">
				<div class="listing-icon-title">
					<div class="title-bottom"><%= listing.title %>
					</div>
				</div>
				<div class="bottom">
					<div class="listing-icon-price">
						$<%= listing.price %>
					</div>
					<div class"listing-icon-permonth">
						Per Month
					</div>
				</div>
			</div>
		</div>
	</div>
</a>